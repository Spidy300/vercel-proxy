{
  "version": 2,
  "functions": {
    "api/proxy.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/proxy",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/",
      "dest": "/api/proxy.js"
    }
  ],
  "headers": [
    {
      "source": "/api/proxy",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Range, Content-Type, Accept, Accept-Encoding, Origin, Referer"
        }
      ]
    }
  ]
}
